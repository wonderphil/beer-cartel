<% provide(:title, 'Add Beer') %>
<div class="boxed">
	<h1>Add Beer</h1>
	<div class="row">
	  <div class="col-md-6 col-md-offset-3">
	    <%= form_for(@beer, html: { multipart: true }) do |f| %>
	      <%= render 'fields', f: f %>
	      
	      <%= f.submit "Create beer", class: "btn btn-primary" %>
	      <%= link_to 'Cancel', beers_path, class: "btn btn-danger" %>
	    <% end %>
	  </div>
	</div>
</div>
