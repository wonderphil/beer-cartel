<% provide(:title, "Edit Review") %>
<div class="boxed">
	<h1>Update your Review</h1>
	<div class="row">
	  <div class="col-md-6 col-md-offset-3">
	    <%= form_for([@beer, @review]) do |f| %>
	      <%= render 'fields', f: f %>
	      <%= f.submit "Save changes", class: "btn btn-primary" %>
	      <%= link_to 'Cancel', beer_path(@beer), class: "btn btn-danger" %>
	    <% end %>
	  </div>
	</div>
</div>